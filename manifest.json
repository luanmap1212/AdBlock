{
    "manifest_version": 2,
    "name": "LT Ad Blocker",
    "version": "1.11", 
    "description": "Adblocker that doesn't get detected",
    "icons": {
        "64": "icon64.png",
        "128": "icon128.png",
        "256": "icon256.png"
    },
    "background": {
        "scripts": ["blockedsites.js", "LTadblock.js"]
    },
    "permissions": ["webRequest", "webRequestBlocking", "http://*/", "https://*/"]
}